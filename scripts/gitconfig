[alias]
    s = switch
    ss = stash
    ru = remote update
    rs = reset
    rb = rebase
    pl = pull origin HEAD
    ps = push origin HEAD
    l = log
    bs = branch --sort=-committerdate
    px = !bash -c 'git push origin HEAD && npx --yes git-open'
